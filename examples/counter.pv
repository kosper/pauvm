#This is an example of a counter using Pauven Bytecode.
#Comments start with the '#'

#Push the first two values into the stack.
PUSH 0

loop:
  PUSH 1
  ADD
  
  #Add the result to the stack and check if it is equal to 10.
  DUP 
  PUSH 10
  EQ
  
  #If the result is 10 we terminate the program, otherwise we jump back to the loop label.
  JMPZ terminates
  JMP loop

terminates:
  HALT
