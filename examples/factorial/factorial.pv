#This program demonstrates the calculation of 5 factorial.

main:
  PUSH 5
  
  CALL factorial
  HALT

factorial:
  DUP

  #This is not a function call, rather a way to demonstrate jumps.
  JMPZ base_case 
  DUP
  PUSH 1
  MINUS
  CALL factorial
  MUL
  RETURN

base_case:
  POP
  PUSH 1

  #Returns to main(Remember not a function call).
  RETURN
